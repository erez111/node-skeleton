{
  "name": "@erez111/node-skeleton",
  "description": "NodeJS skeleton project. Contains basic packages and configuration for our ideal initial node skeleton. Just easily run node-skeleton cli",
  "version": "0.2.7",
  "main": "build/src/cli.js",
  "scripts": {
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon",
    "copyFiles": "copyfiles -e \"data/node_modules\" -e \"data/.idea\" -e \"data/.vscode\" -e \"data/.npmrc\" -a \"./data/**/*\" \"./build/\"",
    "prepublish": "npm run build && npm run copyFiles",
    "publish": "npm publish --access public"
  },
  "author": "Erez Narotzki",
  "license": "MIT",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "fs-extra": "^9.0.1",
    "get-installed-path": "^4.0.8",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.22",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "concurrently": "^5.2.0",
    "copyfiles": "^2.3.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "preferGlobal": true,
  "bin": {
    "node-skeleton": "build/src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erez111/node-skeleton.git"
  },
  "keywords": [
    "node-skeleton",
    "cli",
    "cli node skeleton",
    "node",
    "project",
    "nodejs",
    "typescript",
    "project",
    "swagger",
    "project",
    "openapi",
    "node initial project",
    "nodejs initial project",
    "example project",
    "node example project",
    "nodejs example project",
    "typescript example project"
  ],
  "bugs": {
    "url": "https://github.com/erez111/node-skeleton/issues"
  },
  "homepage": "https://github.com/erez111/node-skeleton#readme"
}
